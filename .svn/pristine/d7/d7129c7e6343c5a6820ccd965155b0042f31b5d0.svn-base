package vo;

import java.util.Date;

public class PointHistory {
	private int no;	 		  // 포인트 히스토리 고유번호
	private User user;		  // 유저번호
	private int usedPoint;	  // 사용포인트
	private int savedPoint;   // 적립포인트
	private Date createdDate; // 히스토리 생성일자
	private int orderNo;	  // 결제정보의 주문번호
	
	public PointHistory() {}

	public int getNo() {
		return no;
	}

	public void setNo(int no) {
		this.no = no;
	}

	public User getUser() {
		return user;
	}

	public void setUser(User user) {
		this.user = user;
	}

	public int getUsedPoint() {
		return usedPoint;
	}

	public void setUsedPoint(int usedPoint) {
		this.usedPoint = usedPoint;
	}

	public int getSavedPoint() {
		return savedPoint;
	}

	public void setSavedPoint(int savedPoint) {
		this.savedPoint = savedPoint;
	}
	
	public Date getCreatedDate() {
		return createdDate;
	}

	public void setCreatedDate(Date createdDate) {
		this.createdDate = createdDate;
	}
	
	public int getOrderNo() {
		return orderNo;
	}

	public void setOrderNo(int orderNo) {
		this.orderNo = orderNo;
	}

	@Override
	public String toString() {
		return "PointHistory [no=" + no + ", user=" + user + ", usedPoint=" + usedPoint + ", savedPoint=" + savedPoint
				+ ", createdDate=" + createdDate + ", orderNo=" + orderNo + "]";
	}

	
}
