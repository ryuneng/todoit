package vo;

import java.util.Date;

public class MeetingReply {

	private int no;
	private String content;
	private String deleted;
	private Date createdDate;
	private Date updatedDate;
	private int dangerCount;
	private Meeting meeting;
	private User User;
	private int partentsReply;
	public int getNo() {
		return no;
	}
	public void setNo(int no) {
		this.no = no;
	}
	public String getContent() {
		return content;
	}
	public void setContent(String content) {
		this.content = content;
	}
	public String getDeleted() {
		return deleted;
	}
	public void setDeleted(String deleted) {
		this.deleted = deleted;
	}
	public Date getCreatedDate() {
		return createdDate;
	}
	public void setCreatedDate(Date createdDate) {
		this.createdDate = createdDate;
	}
	public Date getUpdatedDate() {
		return updatedDate;
	}
	public void setUpdatedDate(Date updatedDate) {
		this.updatedDate = updatedDate;
	}
	public int getDangerCount() {
		return dangerCount;
	}
	public void setDangerCount(int dangerCount) {
		this.dangerCount = dangerCount;
	}
	public Meeting getMeeting() {
		return meeting;
	}
	public void setMeeting(Meeting meeting) {
		this.meeting = meeting;
	}
	public User getUser() {
		return User;
	}
	public void setUser(User user) {
		User = user;
	}
	public int getPartentsReply() {
		return partentsReply;
	}
	public void setPartentsReply(int partentsReply) {
		this.partentsReply = partentsReply;
	}
	
	@Override
	public String toString() {
		return "MeetingReply [no=" + no + ", content=" + content + ", deleted=" + deleted + ", createdDate="
				+ createdDate + ", updatedDate=" + updatedDate + ", dangerCount=" + dangerCount + ", meeting=" + meeting
				+ ", User=" + User + ", partentsReply=" + partentsReply + "]";
	}
	
}
