package vo;

public class PointHistory {
	private int no;	 		 // 포인트내역 고유번호
	private User user;		 // 유저번호
	private int usedPoint;	 // 사용포인트
	private int savedPoint;  // 적립포인트
	private int beforePoint; // 적립/사용 전 포인트
	private int afterpoint;  // 적립/사용 후 포인트
	
	public PointHistory() {}

	public int getNo() {
		return no;
	}

	public void setNo(int no) {
		this.no = no;
	}

	public User getUser() {
		return user;
	}

	public void setUser(User user) {
		this.user = user;
	}

	public int getUsedPoint() {
		return usedPoint;
	}

	public void setUsedPoint(int usedPoint) {
		this.usedPoint = usedPoint;
	}

	public int getSavedPoint() {
		return savedPoint;
	}

	public void setSavedPoint(int savedPoint) {
		this.savedPoint = savedPoint;
	}

	public int getBeforePoint() {
		return beforePoint;
	}

	public void setBeforePoint(int beforePoint) {
		this.beforePoint = beforePoint;
	}

	public int getAfterpoint() {
		return afterpoint;
	}

	public void setAfterpoint(int afterpoint) {
		this.afterpoint = afterpoint;
	}

	@Override
	public String toString() {
		return "PointHistory [no=" + no + ", user=" + user + ", usedPoint=" + usedPoint + ", savedPoint=" + savedPoint
				+ ", beforePoint=" + beforePoint + ", afterpoint=" + afterpoint + "]";
	}
	
	
}
